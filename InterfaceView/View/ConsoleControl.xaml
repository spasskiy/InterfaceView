<UserControl x:Class="InterfaceView.View.ConsoleControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:InterfaceView.View"
             mc:Ignorable="d" 
             >
    <GroupBox Margin="3" Padding="0 0 0 0" Header="Консоль :" Background="{DynamicResource ViewCotrolBackground}">
        <ListBox x:Name="consoleControl" ItemsSource="{Binding Messages}" />
    </GroupBox>
</UserControl>
